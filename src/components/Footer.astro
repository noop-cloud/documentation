<script>
 const cookieConsent = window.localStorage.getItem('NOOP_COOKIE_CONSENT')
 const modal = document.getElementById('cookie-consent-modal');

 document.getElementById('accept-cookie-button').addEventListener('click', () => {
   window.localStorage.setItem('NOOP_COOKIE_CONSENT', 'ACCEPT')
   modal.classList.add('hidden')
 })

 document.getElementById('reject-cookie-button').addEventListener('click', () => {
   window.localStorage.setItem('NOOP_COOKIE_CONSENT', 'REJECT')
   modal.classList.add('hidden')
 })

 if ((cookieConsent === 'ACCEPT') || (cookieConsent === 'REJECT')) {
   modal.classList.add('hidden')
 }

</script>
